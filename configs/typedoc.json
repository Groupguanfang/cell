{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/*",
    "dev-packages/*",
    "ai-packages/*",
    "cloud-packages/*",
    "ui-packages/*",
    "plugins/*"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/examples/**",
    "**/deprecated/**"
  ],
  "name": "CellJS TypeDoc",
  "entryPointStrategy": "packages",
  "includeVersion": false,
  "hideGenerator": true,
  "excludeNotDocumented": false,
  "out": "../gh-pages/docs/next"
}